URxvt.font: xft:DejaVu Sans Mono for Powerline:size=13.5 
URxvt.depth:                      32
URxvt*scrollBar:                  false
URxvt*mouseWheelScrollPage:       false
URxvt*cursorBlink:                true
URxvt*saveLines:                  5000
URxvt*internalBorder: 5
URxvt*geometry: 70x19

rofi.color-enabled: true
rofi.color-window: #282A36, #d96468, #d96468
rofi.color-normal: #282A36, #c5c5c5, #282A36, #282A36, #9a64d9
rofi.color-active: #282A36, #c5c5c5, #282A36, #282A36, #9a64d9
rofi.color-urgent: #282A36, #c5c5c5, #282A36, #282A36, #9a64d9

rofi.separator-style: solid
rofi.sidebar-mode: false
rofi.lines: 5
rofi.font: Source Code Pro Semibold 11.5
rofi.bw: 1
rofi.columns: 2
rofi.padding: 5
rofi.fixed-num-lines: true
rofi.hide-scrollbar: true
